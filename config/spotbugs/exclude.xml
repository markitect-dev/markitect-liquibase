<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter xmlns="https://github.com/spotbugs/filter/4.8.4"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/spotbugs/filter/4.8.4
        https://raw.githubusercontent.com/spotbugs/spotbugs/4.8.4/spotbugs/etc/findbugsfilter.xsd">
  <Match>
    <Class name="dev.markitect.liquibase.base.PreconditionsTests"/>
    <Method name="~lambda\$checkNotNull_throwsNullPointerException\$\d++"/>
    <Bug pattern="NP_NONNULL_PARAM_VIOLATION"/>
  </Match>
  <Match>
    <Class name="dev.markitect.liquibase.spring.boot.autoconfigure.MarkitectLiquibaseAutoConfigurationTests$MarkitectLiquibaseConfigurationTests"/>
    <Method name="~lambda\$shouldFailToCreateLiquibaseBeanWithNoDataSourceUrlOrLiquibaseUrl\$\d++"/>
    <Bug pattern="RV_RETURN_VALUE_IGNORED"/>
  </Match>
  <Match>
    <Class name="dev.markitect.liquibase.structure.StructuresTests"/>
    <Method name="~lambda\$isCatalogOrSchemaType_throwsNullPointerException\$\d++"/>
    <Or>
      <Bug pattern="NP_NULL_PARAM_DEREF_NONVIRTUAL"/>
      <Bug pattern="RV_RETURN_VALUE_IGNORED"/>
    </Or>
  </Match>
  <Match>
    <Class name="dev.markitect.liquibase.util.StringsTests"/>
    <Method name="~lambda\$isIllegalIdentifier_throwsNullPointerException\$\d++"/>
    <Or>
      <Bug pattern="NP_NULL_PARAM_DEREF_NONVIRTUAL"/>
      <Bug pattern="RV_RETURN_VALUE_IGNORED"/>
    </Or>
  </Match>
  <Match>
    <Source name="~.*Tests\.java"/>
    <Bug pattern="THROWS_METHOD_THROWS_CLAUSE_BASIC_EXCEPTION"/>
  </Match>
</FindBugsFilter>
